  <!--${.::bsp.cpp}-->
  <file name="bsp.cpp">
   <text>#include &quot;qpcpp.hpp&quot;        // QP/C++ real-time embedded framework
#include &quot;bsp.hpp&quot;          // Board Support Package
#include &quot;app.hpp&quot;          // Application

#include &lt;iostream&gt;         // for cout/cerr
#include &lt;cstdlib&gt;          // for exit()

#if (QP_VERSION &lt; 800U)
#error this application requires qpcpp version 8.0.0 or higher
#endif

#ifdef Q_SPY
#error Simple Blinky Application does not support QP/Spy software tracing
#endif

//============================================================================
namespace { // unnamed namespace for local stuff with internal linkage

Q_DEFINE_THIS_FILE

} // unnamed namespace

//============================================================================
extern &quot;C&quot; {

//............................................................................
Q_NORETURN Q_onError(char const * const module, int_t id) {
    std::cerr &lt;&lt; &quot;ERROR in &quot; &lt;&lt; module &lt;&lt; ':' &lt;&lt; id &lt;&lt; std::endl;
    QP::QF::onCleanup();
    QS_EXIT();
    exit(-1);
}

} // extern &quot;C&quot;

//============================================================================
namespace BSP {

//............................................................................
void init(void const * const arg) {
    Q_UNUSED_PAR(arg);

    std::cout &lt;&lt; &quot;Simple Blinky example\n&quot;
            &quot;QP/C++ version: &quot; QP_VERSION_STR &quot;\n&quot;
            &quot;Press Ctrl-C to quit...&quot; &lt;&lt; std::endl;

    // mutable events not used -- no need to call QP::QF::poolInit()
    // publish-subscribe not used -- no need to call QP::QActive::psInit()
}
//............................................................................
void ledOn() {
    std::cout &lt;&lt; &quot;LED-ON&quot; &lt;&lt; std::endl;
}
//............................................................................
void ledOff() {
    std::cout &lt;&lt; &quot;LED-OFF&quot; &lt;&lt; std::endl;
}

} // namespace BSP

//============================================================================
namespace QP {

// QF callbacks...
void QF::onStartup() {
    // start AOs...
    static QEvtPtr blinkyQueueSto[10];
    APP::AO_Blinky-&gt;start(
        1U,                      // QP prio. of the AO
        blinkyQueueSto,          // event queue storage
        Q_DIM(blinkyQueueSto),   // queue length [events]
        nullptr, 0U);            // no stack storage
}
//............................................................................
void QF::onCleanup() {
}
//............................................................................
void QF::onClockTick() {
    QTimeEvt::TICK_X(0U, nullptr);  // QTimeEvt clock tick processing
}

} // namespace QP</text>
