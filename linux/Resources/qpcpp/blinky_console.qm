##############################################################################
#
# examples of invoking this Makefile:
# building configurations: Debug (default), Release, and Spy
# make
# make CONF=rel
# make CONF=spy
# make clean   # cleanup the build
# make CONF=spy clean   # cleanup the build
#
# NOTE:
# To use this Makefile on Windows, you will need the GNU make utility, which
# is included in the QTools collection for Windows, see:
#    https://github.com/QuantumLeaps/qtools

#-----------------------------------------------------------------------------
# project:
#
PROJECT := blinky

VPATH := .

INCLUDES := -I.

C_SRCS :=

CPP_SRCS := \
    main.cpp \
    bsp.cpp \
    blinky.cpp

DEFINES  :=

LIB_DIRS :=
LIBS     :=
OUTPUT   := $(PROJECT)

#-----------------------------------------------------------------------------
# target
#
TARGET   := host

DEFINES +=

BIN_DIR := build_$(TARGET)

#-----------------------------------------------------------------------------
# QP framework
#

# location of the QP framework (if not provided in an env. variable)
ifeq ($(QPCPP),)
QPCPP := ../../..
endif

ifeq ($(OS),Windows_NT)

ifdef QV
QP_PORT_DIR := $(QPCPP)/ports/win32-qv
else
QP_PORT_DIR := $(QPCPP)/ports/win32
endif
LIBS += -lws2_32

else

ifdef QV
QP_PORT_DIR := $(QPCPP)/ports/posix-qv
else
QP_PORT_DIR := $(QPCPP)/ports/posix
endif
LIBS += -lpthread

endif

VPATH += \
    $(QP_PORT_DIR)

INCLUDES += \
    -I$(QPCPP)/include \
    -I$(QP_PORT_DIR)

#-----------------------------------------------------------------------------
# QP framework sources
#
QP_SRCS := \
    qep_hsm.cpp \
    qep_msm.cpp \
    qf_act.cpp \
    qf_actq.cpp \
    qf_defer.cpp \
    qf_dyn.cpp \
    qf_mem.cpp \
    qf_ps.cpp \
    qf_qact.cpp \
    qf_qeq.cpp \
    qf_qmact.cpp \
    qf_time.cpp \
    qf_port.cpp

QS_SRCS := \
    qs.cpp \
    qs_64bit.cpp \
    qs_rx.cpp \
    qs_fp.cpp \
    qs_port.cpp

VPATH += \
    $(QPCPP)/src/qf \
    $(QPCPP)/src/qs \
    $(QP_PORT_DIR)

INCLUDES += \
    -I$(QPCPP)/include \
    -I$(QP_PORT_DIR)

# add the QP sources to the build
CPP_SRCS += $(QP_SRCS)

ifeq (spy, $(CONF))  # Spy configuration
VPATH  += $(QPCPP)/src/qs
CPP_SRCS += $(QS_SRCS)
endif

#-----------------------------------------------------------------------------
# GNU GCC toolset for the host:
# NOTE: GNU toolset for Windows (MinGW) is included in the QTools collection
# for Windows, see: https://www.state-machine.com/qtools
#
# It is assumed that %QTOOLS%\bin directory is added to the PATH
#
CC    := gcc
CPP   := g++
LINK  := g++   # for C++ programs

#-----------------------------------------------------------------------------
# basic utilities (included in QTools for Windows), see:
#     https://www.state-machine.com/qtools
#
ifeq ($(OS),Windows_NT)
    TARGET_EXT := .exe
    MKDIR      := mkdir
    RM         := rm
    CAT        := type
    COV        := gcov -abcgum
    CODE_COV := --coverage -fcondition-coverage  # NOTE: required gcc 14 or newer
else ifeq ($(OSTYPE),cygwin)
    TARGET_EXT := .exe
    MKDIR      := mkdir -p
    RM         := rm -f
    CAT        := cat
    COV        := gcov -b
    CODE_COV := -fprofile-arcs -ftest-coverage   # old gcc (version &lt; 14)
else
    TARGET_EXT :=
    MKDIR      := mkdir -p
    RM         := rm -f
    CAT        := cat
    COV        := gcov -b
    CODE_COV := -fprofile-arcs -ftest-coverage   # old gcc (version &lt; 14)
endif

##############################################################################
# Typically you should not need to change anything below this line

#-----------------------------------------------------------------------------
# build configurations...

ifeq (rel, $(CONF)) # Release configuration ..................................

BIN_DIR := build_rel
# gcc options:
CFLAGS := -c -fno-pie -std=c11 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion \
    -O3 $(INCLUDES) $(DEFINES) $(DEF) -DNDEBUG

CPPFLAGS := -c -fno-pie -std=c++17 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion -Wold-style-cast \
    -fno-rtti -fno-exceptions \
    -O3 $(INCLUDES) $(DEFINES) $(DEF) -DNDEBUG

else ifeq (spy, $(CONF))  # Spy configuration ................................

BIN_DIR := build_spy

# gcc options:
CFLAGS := -c -g -fno-pie -std=c11 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion \
    -O $(INCLUDES) $(DEFINES) $(DEF) -DQ_SPY

CPPFLAGS := -c -g -fno-pie -std=c++17 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion -Wold-style-cast \
    -fno-rtti -fno-exceptions \
    -O $(INCLUDES) $(DEFINES) -DQ_SPY

else # default Debug configuration .........................................

BIN_DIR := build

# gcc options:
CFLAGS := -c -g -fno-pie -std=c11 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion \
    -O $(INCLUDES) $(DEFINES) $(DEF)

CPPFLAGS := -c -g -fno-pie -std=c++17 -pedantic \
    -Wall -Wextra -Wpedantic -Wsign-conversion -Wold-style-cast \
    -fno-rtti -fno-exceptions \
    -O $(INCLUDES) $(DEFINES) $(DEF)

endif  # .....................................................................

ifdef GCOV
    CFLAGS    += $(CODE_COV)
    CPPFLAGS  += $(CODE_COV)
    LINKFLAGS := $(CODE_COV)
endif

ifndef GCC_OLD
    LINKFLAGS += -no-pie
endif

#-----------------------------------------------------------------------------
C_OBJS       := $(patsubst %.c,%.o,   $(C_SRCS))
CPP_OBJS     := $(patsubst %.cpp,%.o, $(CPP_SRCS))

TARGET_EXE   := $(BIN_DIR)/$(PROJECT)$(TARGET_EXT)
C_OBJS_EXT   := $(addprefix $(BIN_DIR)/, $(C_OBJS))
C_DEPS_EXT   := $(patsubst %.o, %.d, $(C_OBJS_EXT))
CPP_OBJS_EXT := $(addprefix $(BIN_DIR)/, $(CPP_OBJS))
CPP_DEPS_EXT := $(patsubst %.o, %.d, $(CPP_OBJS_EXT))

# create $(BIN_DIR) if it does not exist
ifeq (&quot;$(wildcard $(BIN_DIR))&quot;,&quot;&quot;)
$(shell $(MKDIR) $(BIN_DIR))
endif

#-----------------------------------------------------------------------------
# rules

.PHONY: clean show

all: $(TARGET_EXE)

ifeq (spy, $(CONF))

$(TARGET_EXE) : $(C_OBJS_EXT) $(CPP_OBJS_EXT)
    $(CPP) $(CPPFLAGS) $(QPCPP)/src/qs/qstamp.cpp -o $(BIN_DIR)/qstamp.o
    $(LINK) $(LINKFLAGS) $(LIB_DIRS) -o $@ $^ $(BIN_DIR)/qstamp.o $(LIBS)

else

$(TARGET_EXE) : $(C_OBJS_EXT) $(CPP_OBJS_EXT)
    $(LINK) $(LINKFLAGS) $(LIB_DIRS) -o $@ $^ $(LIBS)

endif

$(BIN_DIR)/%.d : %.c
    $(CC) -MM -MT $(@:.d=.o) $(CFLAGS) $&lt; &gt; $@

$(BIN_DIR)/%.d : %.cpp
    $(CPP) -MM -MT $(@:.d=.o) $(CPPFLAGS) $&lt; &gt; $@

$(BIN_DIR)/%.o : %.c
    $(CC) $(CFLAGS) $&lt; -o $@

$(BIN_DIR)/%.o : %.cpp
    $(CPP) $(CPPFLAGS) $&lt; -o $@

# create BIN_DIR and include dependencies only if needed
ifneq ($(MAKECMDGOALS),clean)
  ifneq ($(MAKECMDGOALS),show)
   ifneq ($(MAKECMDGOALS),debug)
ifeq (&quot;$(wildcard $(BIN_DIR))&quot;,&quot;&quot;)
$(shell $(MKDIR) $(BIN_DIR))
endif
-include $(C_DEPS_EXT) $(CPP_DEPS_EXT)
   endif
  endif
endif

clean :
    -$(RM) $(BIN_DIR)/*.*

show :
    @echo PROJECT      = $(PROJECT)
    @echo TARGET_EXE   = $(TARGET_EXE)
    @echo MAKECMDGOALS = $(MAKECMDGOALS)
    @echo VPATH        = $(VPATH)
    @echo INCLUDES     = $(INCLUDES)
    @echo DEFINES      = $(DEFINES)
    @echo DEF          = $(DEF)
    @echo C_SRCS       = $(C_SRCS)
    @echo CPP_SRCS     = $(CPP_SRCS)
    @echo C_DEPS_EXT   = $(C_DEPS_EXT)
    @echo C_OBJS_EXT   = $(C_OBJS_EXT)
    @echo CPP_DEPS_EXT = $(CPP_DEPS_EXT)
    @echo CPP_OBJS_EXT = $(CPP_OBJS_EXT)
    @echo LIB_DIRS     = $(LIB_DIRS)
    @echo LIBS         = $(LIBS)
    @echo DEFINES      = $(DEFINES)
    @echo QTOOLS       = $(QTOOLS)
</text>
  </file>
 </directory>
