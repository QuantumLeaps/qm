else
QP_PORT_DIR := $(QPCPP)/ports/posix
endif
LIBS += -lpthread

endif

VPATH += \
    $(QP_PORT_DIR)

INCLUDES += \
    -I$(QPCPP)/include \
    -I$(QP_PORT_DIR)

#-----------------------------------------------------------------------------
# QP framework sources
#
QP_SRCS := \
    qep_hsm.cpp \
    qep_msm.cpp \
    qf_act.cpp \
    qf_actq.cpp \
    qf_defer.cpp \
    qf_dyn.cpp \
    qf_mem.cpp \
    qf_ps.cpp \
    qf_qact.cpp \
    qf_qeq.cpp \
    qf_qmact.cpp \
    qf_time.cpp \
    qf_port.cpp

QS_SRCS := \
    qs.cpp \
    qs_64bit.cpp \
    qs_rx.cpp \
    qs_fp.cpp \
    qs_port.cpp

VPATH += \
    $(QPCPP)/src/qf \
    $(QPCPP)/src/qs \
    $(QP_PORT_DIR)

INCLUDES += \
    -I$(QPCPP)/include \
    -I$(QP_PORT_DIR)

# add the QP sources to the build
CPP_SRCS += $(QP_SRCS)

ifeq (spy, $(CONF))  # Spy configuration
VPATH  += $(QPCPP)/src/qs
CPP_SRCS += $(QS_SRCS)
endif

#-----------------------------------------------------------------------------
# GNU GCC toolset for the host:
# NOTE: GNU toolset for Windows (MinGW) is included in the QTools collection
# for Windows, see: https://www.state-machine.com/qtools
#
# It is assumed that %QTOOLS%\bin directory is added to the PATH
#
CC    := gcc
CPP   := g++
LINK  := g++   # for C++ programs

#-----------------------------------------------------------------------------
# basic utilities (included in QTools for Windows), see:
#     https://www.state-machine.com/qtools
#
ifeq ($(OS),Windows_NT)
    TARGET_EXT := .exe
    MKDIR      := mkdir
    RM         := rm
    CAT        := type
    COV        := gcov -abcgum
    CODE_COV := --coverage -fcondition-coverage  # NOTE: required gcc 14 or newer
else ifeq ($(OSTYPE),cygwin)
    TARGET_EXT := .exe
    MKDIR      := mkdir -p
    RM         := rm -f
    CAT        := cat
    COV        := gcov -b
    CODE_COV := -fprofile-arcs -ftest-coverage   # old gcc (version &lt; 14)
else
    TARGET_EXT :=
    MKDIR      := mkdir -p
    RM         := rm -f
    CAT        := cat
    COV        := gcov -b
    CODE_COV := -fprofile-arcs -ftest-coverage   # old gcc (version &lt; 14)
endif

